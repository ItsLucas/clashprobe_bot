INFLUX_URL=http://localhost:8086
INFLUX_TOKEN=replace-me
INFLUX_ORG=replace-me
INFLUX_BUCKET=clashprobe

# Polling configuration
TIME_RANGE_MINUTES=5
POLL_INTERVAL_SECONDS=30
LATENCY_WARN_MS=

# Telegram
TELEGRAM_BOT_TOKEN=replace-me
# Option A: set explicit IDs
# TELEGRAM_CHAT_ID=
# TELEGRAM_MESSAGE_ID=

# Option B: use /init_status in group to bootstrap and persist IDs

# Display
STATUS_TITLE=Network Status
SHOW_PROTOCOL=true
TZ=UTC

# Template selection: 'default' or 'board_zh'
# When using 'board_zh', configure probe nodes below
# STATUS_TEMPLATE=default

# Probe node tags (Influx tag 'node') for board_zh
# DOMESTIC_PROBE_NODE=region-sh-node-aliyun
# FOREIGN_PROBE_NODE=
# INCLUDE_DEGRADED_AS_ALERT=true
